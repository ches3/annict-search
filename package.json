{
	"name": "@ches3/annict-search",
	"version": "0.1.0",
	"description": "AnnictのAPIを使って作品の検索と記録をするライブラリ",
	"author": "ches3",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ches3/annict-search.git"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"test": "vitest",
		"gql-gen": "graphql-codegen",
		"lint": "biome lint --write ./src",
		"format": "biome format --write ./src",
		"check": "biome check --write ./src"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@graphql-codegen/cli": "^5.0.3",
		"@graphql-codegen/typed-document-node": "^5.0.10",
		"@types/bun": "^1.1.12",
		"dotenv": "^16.4.5",
		"tsup": "^8.3.0",
		"typescript": "^5.6.3",
		"vitest": "^2.1.3"
	},
	"dependencies": {
		"@ches3/zenhan": "^1.0.1",
		"@geolonia/japanese-numeral": "^1.0.2",
		"graphql": "^16.9.0",
		"graphql-request": "^7.1.0",
		"romans": "^2.0.16"
	}
}
